programa
{
	
	funcao inicio()
	{
		real custo_final, custo_fabrica

		escreva("digite o custo da fabrica: ")
		leia(custo_fabrica)

		custo_final = custo_fabrica + (custo_fabrica * 0.28) + (custo_fabrica * 0.45)
		escreva("custo final ao cosumidor: ",custo_final,"\n")
		
	}
}

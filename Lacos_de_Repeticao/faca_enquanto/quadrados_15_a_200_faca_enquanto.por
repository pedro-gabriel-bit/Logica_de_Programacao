programa
{
	funcao inicio()
	{
		inteiro n, quadrado
		n = 15

		faca
		{
			quadrado = n * n
			escreva("Numero: ", n, "  Quadrado: ", quadrado, "\n")
			n = n + 1
		}
		enquanto (n <= 200)
	}
}

programa
{
	
	funcao inicio()
	{
		inteiro A, B


		escreva("escreva o valor de A: ")
		leia(A)

		escreva("escreva o valor de  B: ")
		leia(B)

		se (A >= B){
		escreva(" A e maior a B","/n")
		}
			
		senao {
		escreva(" B e maior que A","/n")
		}

		
		
	}
}

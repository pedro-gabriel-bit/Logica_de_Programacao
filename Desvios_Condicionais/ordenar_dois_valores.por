programa
{
	
	funcao inicio()
	{
		inteiro valor1, valor2

		escreva("digite o valor1: ")
		leia(valor1)

		escreva("digite o valor2: ")
		leia(valor2)


		se (valor1 < valor2){
		escreva(valor1, ", ", valor1)
		}

		senao{
			escreva(valor2, ", ", valor1)
		}

		
	}
}



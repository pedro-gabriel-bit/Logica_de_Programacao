programa
{
	
	funcao inicio()
	{
		real valor

		escreva("digite um valor: ")
		leia(valor)

		se (valor >= 0){
		escreva(" o valor e positico")
		}
		senao{
		escreva("o valor e negativo")
		}
	}
}

